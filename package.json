{
  "name": "npm-otp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev:client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev:server": "nodemon --exec \"node -r dotenv/config server/bin/www -w server\"",
    "dev": "run-p dev:*",
    "start": "node server/bin/www",
    "heroku-postbuild": "npm run build",
    "prettier": "prettier --write '**/*.js?(on)' '**/*.scss'"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-promise-router": "^3.0.3",
    "firebase": "^5.8.2",
    "firebase-admin": "^7.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "morgan": "~1.9.0",
    "otplib": "^10.0.1",
    "prettier": "^1.16.4",
    "qr-scanner": "^1.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-firebaseui": "^3.1.2",
    "react-qr-reader": "^2.1.2",
    "react-scripts": "2.1.3",
    "uniqid": "^5.0.3",
    "url-otpauth": "^2.0.0",
    "web-push": "^3.3.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{server,src}/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "proxy": "http://localhost:3001"
}
